--made by just a tree#0556

--version

local version = "0.2.0 ALPHA"

--main script

repeat wait() until game:IsLoaded()

if not getgenv then warn("BAD EXECUTOR") return end
if not game.GameId == 3620011279 then warn("INVALID GAME") return end
if getgenv().EXISTS == true then warn("ALREADY EXECUTED") return end

getgenv().EXISTS  = true

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-ui-Library/main/source.lua"))()
local name = "raise a floppa 2 raper lol v"..version
local ui = library.new(name)

local settings = {
    ["Auto clicker"] = false;
    ["Auto shit cleaner"] = false;
}

local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),  
	TextColor = Color3.fromRGB(255, 255, 255)
}

local floppa = workspace:WaitForChild("Floppa")
local clickdetector = floppa:WaitForChild("ClickDetector")

local p1 = ui:addPage("Auto")
local p2 = ui:addPage("UI")

local p1s1 = p1:addSection("Section 1")

p1s1:addToggle("Auto clicker", false, function(value)
    
    settings["Auto clicker"] = value
    
    if value == true then
        
        repeat
            
            wait(0.05)
            fireclickdetector(clickdetector)
            
        until settings["Auto clicker"] == false
        
    end
    
end)

p1s1:addToggle("Auto shit cleaner", false, function(value)
    
    settings["Auto shit cleaner"] = value
    
    if value == true then
        
        for _, v in next, workspace:GetDescendants() do
            
            if v.Name == "Poop" and v.Parent.Name == "Workspace" or v.Name == "Poop" and v.Parent.Name == "Litter Box" then
                
                if v.ClassName ~= "Model" then return end
                
                repeat wait() until v:FindFirstChild("ProximityPrompt") and v.PrimaryPart
                
                local oldpos = game.Players.LocalPlayer.Character:GetPrimaryPartCFrame()
                
                game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(v:GetPrimaryPartCFrame())
                
                wait(0.1)
            
                fireproximityprompt(v:FindFirstChild("ProximityPrompt"), 0)
                
                wait(0.1)
                
                game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(oldpos)
                
            end
            
        end
        
    end
    
end)

local p2s1 = p2:addSection("Theme")

for theme, color in pairs(themes) do
	p2s1:addColorPicker(theme, color, function(color3)
		ui:setTheme(theme, color3)
	end)
end

local p2s2 = p2:addSection("Keybinds")

p2s2:addKeybind("Toggle UI", Enum.KeyCode.Semicolon, function()
    
    ui:toggle()
    
end)

local p2s3 = p2:addSection("Destroy UI")

p2s3:addButton("Destroy UI", function()
    
    for _, v in pairs(settings) do
        
        v = false
        
    end
    getgenv().EXISTS = false
    game.CoreGui[name]:Destroy()
    
end)

workspace.DescendantAdded:Connect(function(v)
    
    if settings["Auto shit cleaner"] == true then
        
        if v.Name == "Poop" and v.Parent.Name == "Workspace" or v.Name == "Poop" and v.Parent.Name == "Litter Box" then
            
            if v.ClassName ~= "Model" then return end
            
            repeat wait() until v:FindFirstChild("ProximityPrompt") and v.PrimaryPart
            
            local oldpos = game.Players.LocalPlayer.Character:GetPrimaryPartCFrame()
            
            game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(v:GetPrimaryPartCFrame())
            
            wait(0.1)
            
            fireproximityprompt(v:FindFirstChild("ProximityPrompt"), 0)
            
            wait(0.1)
            
            game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(oldpos)
            
        end
        
    end
    
end)

ui:Notify("Thx for using", "Made by just a tree#0556")

ui:SelectPage(ui.pages[1], true)

